{
  "name": "Twilio Fax Twimlet",
  "description": "A tiny app for receiving faxes with Twilio",
  "repository": "https://github.com/peterkeen/fax-twimlet",
  "keywords": ["ruby", "twilio"],
  "env": {
      "SMTP_SERVER_ADDRESS": {
          "description": "Address of SMTP server",
          "required": true
      },
      "SMTP_SERVER_DOMAIN": {
          "description": "HELO domain to use when sending email",
          "required": true
      },
      "SMTP_SERVER_PORT": {
          "description": "SMTP server port",
          "required": true
      },
      "SMTP_SERVER_USERNAME": {
          "description": "SMTP username",
          "required": true
      },
      "SMTP_SERVER_PASSWORD": {
          "description": "SMTP password",
          "required": true
      },
      "SMTP_SERVER_FROM": {
          "description": "email address to send as",
          "required": true
      }
  }
}
